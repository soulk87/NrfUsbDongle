{
  "configurations": [
    {
      "name": "windows-gcc-x86",
      "includePath": [
        "${workspaceFolder}/**"
      ],
      "defines": [
        "_DEBUG",
        "UNICODE",
        "_UNICODE",
        "VIA_ENABLE",
        "RAW_ENABLE",
        "DYNAMIC_KEYMAP_ENABLE",
        "KEY_OVERRIDE_ENABLE",
        "HOLD_ON_OTHER_KEY_PRESS",
        "EXTRAKEY_ENABLE",
        "MOUSEKEY_ENABLE",
        "MOUSE_ENABLE",
        "POINTING_DEVICE_ENABLE",
        "RF_DONGLE_MODE_ENABLE"
      ],
      "windowsSdkVersion": "10.0.22621.0",
      "compilerPath": "C:/MinGW/bin/gcc.exe",
      "cStandard": "${default}",
      "cppStandard": "${default}",
      "intelliSenseMode": "windows-gcc-x86",
      "configurationProvider": "nordic-semiconductor.nrf-connect"
    }
  ],
  "version": 4
}